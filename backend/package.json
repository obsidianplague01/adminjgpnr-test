{
  "name": "jgpnr-backend",
  "version": "2.0.0",
  "description": "JGPNR Paintball Backend with Advanced Analytics",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "queue:dashboard": "bull-board",
    "logs": "tail -f logs/combined.log"
  },
  "keywords": [
    "ticketing",
    "paintball",
    "analytics",
    "nodejs",
    "typescript"
  ],
  "author": "JGPNR",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^1.3.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.15.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsdom": "^23.2.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.6.1",
    "speakeasy": "^2.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^27.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.4",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
